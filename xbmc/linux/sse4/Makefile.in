ARCH=@ARCH@
DEFINES+=
CXXFLAGS=-fPIC -msse4.1
LIBNAME=libsse4
SRCS=CopyFrame.cpp

LIB_SHARED=@abs_top_srcdir@/system/$(LIBNAME)-$(ARCH).so

all: $(LIB_SHARED)

include @abs_top_builddir@/Makefile.include

$(LIB_SHARED): $(OBJS)
	$(CXX) $(CXXFLAGS) $(LDFLAGS) -shared -o $(LIB_SHARED) $(OBJS)

CLEAN_FILES = \
	$(LIB_SHARED) \

DISTCLEAN_FILES= \
	Makefile \
